{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "aether",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "description": "ðŸ¥š a pterodactyl multiegg",
  "favicon": "/favicon.svg",
  "thumbnails": {
    "appearance": "dark",
    "fonts": {
      "family": "Geist Mono"
    }
  },
  "styling": {
    "eyebrows": "section",
    "codeblocks": {
      "theme": "catppuccin-mocha"
    }
  },
  "icons": {
    "library": "lucide"
  },
  "fonts": {
    "heading": {
      "family": "Geist Mono"
    }
  },
  "appearance": {
    "default": "dark",
    "strict": true
  },
  "background": {
    "decoration": "windows"
  },
  "navbar": {
    "links": [
      {
        "label": "Dashboard",
        "href": "https://dashboard.plexdevelopment.net"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Website",
      "href": "https://plexdevelopment.net"
    }
  },
  "navigation": {
    "dropdowns": [
      {
        "dropdown": "Documentation",
        "icon": "book-open",
        "groups": [
          {
            "group": "Get started",
            "icon": "rocket",
            "pages": [
              "index"
            ]
          }
        ]
      },
      {
        "dropdown": "API reference",
        "icon": "square-terminal",
        "groups": [
          {
            "group": "Core endpoints",
            "icon": "square-terminal",
            "pages": [
              "api-reference/get",
              "api-reference/post",
              "api-reference/delete"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://mintlify.com/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Blog",
          "href": "https://mintlify.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/lonersoft/aether"
    },
    "links": [
      {
        "header": "aether",
        "items": [
          {
            "href": "https://github.com/lonersoft/aether",
            "label": "GitHub Repository"
          },
          {
            "href": "https://github.com/lonersoft/aether-front",
            "label": "Docs Repository"
          }
        ]
      },
      {
        "header": "Donate",
        "items": [
          {
            "href": "https://hcb.hackclub.com/donations/start/lonersoft",
            "label": "Donate via HCB"
          },
          {
            "href": "https://hcb.hackclub.com/lonersoft/transactions",
            "label": "Transparent finances"
          }
        ]
      }
    ]
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  }
}
